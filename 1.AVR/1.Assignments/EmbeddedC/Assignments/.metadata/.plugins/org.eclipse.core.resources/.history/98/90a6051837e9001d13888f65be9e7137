unsigned char RightCircularShift(unsigned int InputNumber, int NumberOfShifts, unsigned int * pOutput){
	unsigned char result;
	if(NumberOfShifts > 32){
		result = 0xFF;
	}else{
		*pOutput = InputNumber;
		result = 0;
	}
	return result;
}
