#include <stdio.h>


int main(){
	char str[20];
	int size =0;
	char result[3];

	printf("Welcome to Exercise 9 program, Please enter a text: ");
	fflush(stdout);
	scanf("%s",str);
	for(int i = 0;str[i] != '\0';i++){
		size++;
	}
	result[0] = str[size];
	result[1] = " ";
	result[2] = str[size-1];
	printf("%s",result);

	return 0;
}
