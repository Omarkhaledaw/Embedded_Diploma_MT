#include <stdio.h>


int main(){
	char str[20];
	//int last2,last3;
	char result[3];

	printf("Welcome to Exercise 9 program, Please enter a text: ");
	fflush(stdout);
	scanf("%s",str);
	for(int i = 0;str[i] != '\0';i++){
		if(str[i+2] == '\0'){
			result[0] = str[i+1];
			result[1] = ' ';
			result[2] = str[i];
		}
	}
	printf("%s",result);

	return 0;
}
