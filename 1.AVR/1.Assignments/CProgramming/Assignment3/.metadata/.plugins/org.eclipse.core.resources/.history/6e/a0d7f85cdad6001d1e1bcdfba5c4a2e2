#include <stdio.h>

int removeDuplicates(int arr_old[], int n_old, int arr_new[], int*n_new);

int main(){



	return 0;
}
int removeDuplicates(int arr_old[], int n_old, int arr_new[], int*n_new){
	if(n_old == 0){
		return 0;
	}else{
		*n_new = 0;
		for(int i = 0;i<n_old-1;i++){
			if(arr_old[i] != arr_old[i+1]){
				arr_new[*n_new] = arr_old[i];
				(*n_new)++;
			}
		}
		arr_new[*n_new] = arr_old[i];
		(*n_new)++;

		return 1;
	}
}
