#include <stdio.h>

void bubbleSort(int *,int);

int main(){

	int arr[10]={17,0,2,3,6,8,1,7,11,10,4};
	printf("Welcome to Exercise 3 Program\n");
	for(int i =0 ;i<10;i++){
		printf("%d, ",arr[i]);
	}
	printf("Now After the Bubble sorting\n");
	bubbleSort(arr,10);
	for(int i =0 ;i<10;i++){
		printf("%d, ",arr[i]);
	}
	return 0;
}
