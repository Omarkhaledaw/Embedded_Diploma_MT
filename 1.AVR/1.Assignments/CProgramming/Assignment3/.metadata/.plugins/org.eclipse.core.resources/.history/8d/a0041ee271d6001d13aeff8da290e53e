#include <stdio.h>

int compareStrings(int a[],int b[]);

int main(){
	int a[]={1,2,3,4,5};
	int b[]={6,7,8,9,10};
	int result;

	printf("Welcome to Exercise 19 Program\n");
	result = compareStrings(a,b);
	printf("%d",result);

	return 0;
}
int compareStrings(int a[],int b[]){
	int count1 = 0;
	int count2 = 0;
	int result = 0;

	for(int i = 0;a[i] != '\0';i++){
		count1++;
	}
	for(int i = 0;b[i] != '\0';i++){
		count2++;
	}
	if(count1 == count2){
		result = 0;
	}else{
		result = 1;
	}
	return result;
}
