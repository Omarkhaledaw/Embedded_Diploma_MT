#include <stdio.h>
#include <math.h>

int powerOfTwo(int num);

int main(){

	int num,result;
	printf("Welcome to Exercise 11 program, Please enter a number: ");
	fflush(stdout);
	scanf("%d",&num);
	result = powerOfTwo(num);
	printf("%d",result);

	return 0;
}
int powerOfTwo(int num){
	int result;
	if(1 == num){
		result = 1;
	}else if (0 ==num){
		result = 0;
	}else if(0 == num%2){
		result = 1;
	}else{
		result = 0;
	}
	return result;
}
